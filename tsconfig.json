{
  "compilerOptions": {
   "target": "es2016",                                  
   "module": "commonjs",                                
   "outDir": "dist",                                      
   "esModuleInterop": true,
   "forceConsistentCasingInFileNames": true,
   "strict": true,   
   "skipLibCheck": true, 
    "plugins":  [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ]
  }
}
