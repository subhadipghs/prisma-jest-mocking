{
  "name": "prisma-jest-mocking",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "seeds": "ts-node ./prisma/seed.ts",
    "migrate": "prisma migrate --name init",
    "migrate:dev": "prisma migrate dev --name init",
    "generate": "prisma generate",
    "start:dev": "ts-node main.ts",
    "ts:init": "npx tsc --init",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "primsa": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "prisma": "^4.2.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.5",
    "jest": "^28.1.3",
    "jest-ts-auto-mock": "^2.1.0",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.13"
  }
}
